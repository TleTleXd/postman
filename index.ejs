<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css">
</head>
<body>
    <%- include('partials/navbar') %>
    <div class="content">
        <h1>Welcome to the Home Page</h1>
        <div class="card-container">
            <% users.forEach(user => { %>
                <div class="card">
                    <h2><%= user.name %></h2>
                    <p>Email: <%= user.email %></p>
                    <p>Phone: <%= user.phone %></p>
                </div>
            <% }) %>
        </div>
    </div>
    <%- include('partials/footer') %>
</body>
</html>
